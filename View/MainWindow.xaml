<Window x:Class="NavrhoveVzoryUkol.MainWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:NavrhoveVzoryUkol"
        mc:Ignorable="d"
        Title="MainWindow" Height="450" Width="350">
    <StackPanel>
        <Label Content="Jmeno" HorizontalContentAlignment="Center" Margin="5 0"></Label>
        <TextBox Margin="4 5" x:Name="JmenoField"></TextBox>
        <Label Content="Prijmeni" HorizontalContentAlignment="Center" Margin="5 0"></Label>
        <TextBox Margin="4 5" x:Name="PrijmeniField"></TextBox>
        <Label Content="Rok Narozeni" HorizontalContentAlignment="Center" Margin="5 0"></Label>
        <TextBox Margin="4 5" x:Name="RokNarozeniField"></TextBox>
        <Label Content="Rodne Cislo" HorizontalContentAlignment="Center" Margin="5 0"></Label>
        <TextBox Margin="4 5" x:Name="RodneCisloField" ></TextBox>
        <Button Content="Vlozit do DB" Margin="4 5" x:Name="Tlacitko" Click="Tlacitko_Click"></Button>
    </StackPanel>
</Window>
